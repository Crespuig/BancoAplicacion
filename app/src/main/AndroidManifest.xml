<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hecrpu.simarro.bancoaplicacion">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Vankio"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TransferActivity"></activity>
        <activity android:name=".CambiarClave" />
        <activity android:name=".PrincipalActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DatosCuenta"
            android:parentActivityName=".TransferActivity"></activity>
        <meta-data android:name="android.support.PARENT_ACTIVITY"
            android:value=".TransferActivity"></meta-data>
    </application>

</manifest>